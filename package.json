{
  "name": "@metafam/achievmintry",
  "version": "1.0.0",
  "keywords": [
    "ethereum",
    "react",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "subgraph:auth": "yarn workspace @achievmintry/subgraph auth",
    "subgraph:codegen": "yarn workspace @achievmintry/subgraph codegen",
    "subgraph:build": "yarn workspace @achievmintry/subgraph build",
    "subgraph:deploy": "yarn workspace @achievmintry/subgraph deploy",
    "dapp:build": "yarn workspace @achievmintry/dapp build",
    "dapp:eject": "yarn workspace @achievmintry/dapp eject",
    "dapp:ipfs": "yarn workspace @achievmintry/dapp ipfs",
    "dapp:start": "yarn workspace @achievmintry/dapp start",
    "dapp:test": "yarn workspace @achievmintry/dapp test"
  },
  "workspaces": {
    "nohoist": [
      "**/@graphprotocol/graph-ts",
      "**/@graphprotocol/graph-ts/**"
    ],
    "packages": [
      "packages/*"
    ]
  }
}
