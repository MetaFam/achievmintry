{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "lib": ["esnext"],
        "moduleResolution": "node",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react",
        "skipLibCheck": true
    },
    "exclude": [
        "**/node_modules",
        "packages/subgraph/**",
        "packages/**/tests/**",
        "packages/**/types/**",
        "packages/**/dist/**",
        "packages/**/build/**",
        "packages/**/out/**"
    ]
}
